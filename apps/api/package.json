{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/http/server.js",
    "build": "tsup",
    "dev": "node --env-file ../../.env --experimental-strip-types --no-warnings --watch src/server.ts",
    "db:generate": "pnpm env:load drizzle-kit generate",
    "db:migrate": "pnpm env:load drizzle-kit migrate",
    "db:seed": "pnpm env:load node --experimental-strip-types --no-warnings src/db/seed.ts",
    "db:ui": "pnpm env:load drizzle-kit studio",
    "env:load": "dotenv -e ../../.env --"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20",
    "@types/pg": "^8.15.4",
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "^0.31.4",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "@fastify/jwt": "^9.1.0",
    "@fastify/multipart": "^9.0.3",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@google/genai": "^1.8.0",
    "@livo/env": "workspace:*",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.62.0",
    "@opentelemetry/sdk-node": "^0.203.0",
    "@opentelemetry/sdk-trace-node": "^2.0.1",
    "drizzle-orm": "^0.44.2",
    "fastify": "^5.4.0",
    "fastify-plugin": "^5.0.1",
    "fastify-type-provider-zod": "^5.0.2",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "zod": "^3.25.76"
  }
